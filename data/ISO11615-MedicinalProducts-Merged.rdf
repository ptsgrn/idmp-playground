    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/isDerivedFromOrganismNamed -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/isDerivedFromOrganismNamed">
        <rdfs:domain rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/SourceMaterialRole"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://www.omg.org/spec/Commons/QuantitiesAndUnits/isDerivedFrom"/>
            <rdf:Description rdf:about="https://www.omg.org/spec/Commons/Designators/hasName"/>
        </owl:propertyChainAxiom>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/"/>
        <rdfs:label>is derived from organism named</rdfs:label>
        <skos:definition>relates source material to an accepted scientific name associated with the source material parent organism based on the organism taxonomy</skos:definition>
