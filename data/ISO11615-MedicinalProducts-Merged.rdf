        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/StatisticalMeasures/"/>
        <rdfs:label>has weight</rdfs:label>
        <skos:definition>indicates an amount given to increase or decrease the importance of an item</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.omg.org/spec/Commons/StatisticalMeasures/isCalculatedViaMethodology -->


    <owl:DatatypeProperty rdf:about="https://www.omg.org/spec/Commons/StatisticalMeasures/isCalculatedViaMethodology">
