        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/Commons/RolesAndCompositions/isPlayedBy"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.omg.org/spec/Commons/Collections/hasMember"/>
                        <owl:someValuesFrom rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/Party"/>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/Organizations/"/>
        <rdfs:label>member-bearing organization</rdfs:label>
        <skos:definition>role of a group or organization that has members that are people or other organizations</skos:definition>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/Commons/Organizations/Membership -->


    <owl:Class rdf:about="https://www.omg.org/spec/Commons/Organizations/Membership">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/Situation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/Commons/Organizations/hasMembership"/>
                <owl:someValuesFrom rdf:resource="https://www.omg.org/spec/Commons/Organizations/MemberBearingOrganization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/Commons/Organizations/hasOrganizationMember"/>
