    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/isDerivedFromOrganismFraction -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/isDerivedFromOrganismFraction">
