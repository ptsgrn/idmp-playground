    <!-- https://www.omg.org/spec/Commons/RegulatoryAgencies/RegulatoryAgency -->


    <owl:Class rdf:about="https://www.omg.org/spec/Commons/RegulatoryAgencies/RegulatoryAgency">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/Actor"/>
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/Commons/ProductsAndServices/ServiceProvider"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/Commons/RegulatoryAgencies/hasJurisdiction"/>
