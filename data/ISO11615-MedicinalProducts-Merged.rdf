        <skos:definition>high-level description of the approach taken to obtain the result</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.omg.org/spec/Commons/StatisticalMeasures/isEstimate -->


    <owl:DatatypeProperty rdf:about="https://www.omg.org/spec/Commons/StatisticalMeasures/isEstimate">
        <rdfs:domain rdf:resource="https://www.omg.org/spec/Commons/StatisticalMeasures/StatisticalMeasure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/StatisticalMeasures/"/>
        <rdfs:label>is estimate</rdfs:label>
        <skos:definition>indicates whether the measure reflects an estimate (approximation) or not</skos:definition>
    </owl:DatatypeProperty>
    


