    </owl:NamedIndividual>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/Preliminary -->


    <owl:NamedIndividual rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/Preliminary">
