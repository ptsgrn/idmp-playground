    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/isRelatedSubstanceTo -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/isRelatedSubstanceTo">
