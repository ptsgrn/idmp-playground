    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasResidueSite -->


    <owl:DatatypeProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasResidueSite">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/TextDatatype/hasTextValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/"/>
