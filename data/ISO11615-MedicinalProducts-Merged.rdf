            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasQuantityValue"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:someValuesFrom>
                    <owl:Class>
