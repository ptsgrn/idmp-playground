        <rdfs:range rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/ContainerSpecification"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/"/>
        <rdfs:label>is contained in</rdfs:label>
        <skos:definition>is held within</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isContraindicationFor -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isContraindicationFor">
