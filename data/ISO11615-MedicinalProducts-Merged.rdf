        <skos:note>The role of the moiety should be specified if there is a specific role the moiety is playing. In addition to the moiety role for chemicals, for many peptides, the moiety role of counter-ions such as acetate should be indicated.</skos:note>
        <idmp-sub:hasConformanceLevel rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/ConformanceLevel-Conditional"/>
        <idmp-dtp:isEncodedAs rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO21090-HarmonizedDatatypes/DatatypeCode-CD"/>
    </owl:Class>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/MolecularComposition -->


    <owl:Class rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/MolecularComposition">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/Commons/RolesAndCompositions/Composition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/Commons/Designators/defines"/>
                <owl:someValuesFrom rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/Substance"/>
