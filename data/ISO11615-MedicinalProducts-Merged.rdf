    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO21090-HarmonizedDatatypes/hasBooleanValue -->


    <owl:DatatypeProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO21090-HarmonizedDatatypes/hasBooleanValue">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
