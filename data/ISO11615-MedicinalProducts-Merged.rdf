    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/hasComorbidity">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/Classifiers/isClassifiedBy"/>
        <rdfs:domain>
            <owl:Class>
