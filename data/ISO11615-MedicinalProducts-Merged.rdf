        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/Commons/Collections/Collection"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/Commons/Designators/isSignifiedBy"/>
