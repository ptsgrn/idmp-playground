        <rdfs:domain rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/ContainerConstituent"/>
        <rdfs:range rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/ManufacturedItem"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/"/>
        <rdfs:label>is characterized by manufactured item</rdfs:label>
        <skos:definition>relates the container constituent to its manufactured item</skos:definition>
        <skos:example>A blister contains 20 tablets. One container constituent of the blister has the quantity of 10 tablets (units) chracterized by the tablet manufactured item.</skos:example>
    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isCharacterizedByPackageItem -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isCharacterizedByPackageItem">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/Classifiers/isCharacterizedBy"/>
        <rdfs:domain rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/ContainerConstituent"/>
        <rdfs:range rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/PackageItem"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/"/>
        <rdfs:label>is characterized by package item</rdfs:label>
        <skos:definition>relates the container constituent to its package item</skos:definition>
        <skos:example>A box contains 5 blisters. One container constituent of the box has the quantity of 5 blisters (units) characterized by a blister package item.</skos:example>
    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isContainedIn -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isContainedIn">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/Collections/isPartOf"/>
