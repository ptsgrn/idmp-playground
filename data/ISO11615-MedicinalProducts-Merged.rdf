    </owl:DatatypeProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasLatitude -->


    <owl:DatatypeProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasLatitude">
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">-90.0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">90.0</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <dcterms:source rdf:resource="http://edndoc.esri.com/arcsde/9.1/general_topics/what_coord_sys.htm"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>has latitude</rdfs:label>
        <skos:definition>the angle formed by the intersection of a line perpendicular to the Earth&apos;s surface at a point and the plane of the Equator</skos:definition>
        <skos:note>Points north of the Equator have positive latitude values, while points south have negative values. Latitude values range from minus 90 to plus 90 degrees. Lines of latitude are also called parallels because a particular value of latitude forms a circle parallel to the Equator.</skos:note>
    </owl:DatatypeProperty>
    


