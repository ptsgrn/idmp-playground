        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/Commons/Collections/isPartOf"/>
                <owl:allValuesFrom rdf:resource="https://www.omg.org/spec/Commons/Organizations/Organization"/>
