    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasInChIKeyValue -->


    <owl:DatatypeProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasInChIKeyValue">
        <rdfs:subPropertyOf rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasStructuralRepresentationText"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
