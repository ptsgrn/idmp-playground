    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasPropertySubstanceIdentifier -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasPropertySubstanceIdentifier">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/Documents/specifies"/>
        <rdfs:domain rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/PropertyOfSubstance"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/SubstanceCode"/>
                    <rdf:Description rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/SubstanceIdentifier"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <dcterms:source>ISO 11238:2018 Health informatics - Identification of medicinal products (IDMP) - Data elements and structures for the unique identification and exchange of regulated information on substances, figures 19-24</dcterms:source>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/"/>
        <rdfs:label>has property substance identifier</rdfs:label>
        <skos:definition>indicates the substance identifier of a substance upon which a defining property depends</skos:definition>
        <skos:example>Solubility in Water, the Substance ID for Water shall be specified.</skos:example>
        <skos:note>The identifier of the substance related to a defining property shall be described where applicable.</skos:note>
        <idmp-sub:hasConformanceLevel rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/ConformanceLevel-Conditional"/>
        <cmns-av:adaptedFrom>ISO/TS 19844:2018(E) Health informatics - Identification of medicinal products (IDMP) - Implementation guidelines for ISO 11238 for data elements and structures for the unique identification and exchange of regulated information on substances, clause 6.11</cmns-av:adaptedFrom>
    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasPropertySubstanceName -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasPropertySubstanceName">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/Documents/specifies"/>
        <rdfs:domain rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/PropertyOfSubstance"/>
        <rdfs:range rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/SubstanceName"/>
        <dcterms:source>ISO 11238:2018 Health informatics - Identification of medicinal products (IDMP) - Data elements and structures for the unique identification and exchange of regulated information on substances, figures 19-24</dcterms:source>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/"/>
