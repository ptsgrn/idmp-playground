    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/TaxonomicClass -->


    <owl:Class rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/TaxonomicClass">
