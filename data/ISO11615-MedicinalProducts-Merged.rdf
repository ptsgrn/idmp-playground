        <rdfs:label>is evaluated by</rdfs:label>
        <skos:definition>is ascertained or determined by</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isImmediatelyContainedIn -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isImmediatelyContainedIn">
        <rdfs:subPropertyOf rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isContainedIn"/>
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/Collections/isDirectPartOf"/>
        <rdfs:range rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/ContainerSpecification"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/"/>
        <rdfs:label>is immediately contained in</rdfs:label>
        <skos:definition>is specified to hold within its volume or area and be in direct contact with</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isInputTo -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isInputTo">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/RegistrationAuthorities/isRelatedTo"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/"/>
        <rdfs:label>is input to</rdfs:label>
        <skos:definition>is put in, taken in, or operated on by</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isInteractionFor -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isInteractionFor">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/Documents/specifies"/>
        <rdfs:domain rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/Interaction"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/"/>
        <rdfs:label>is interaction for</rdfs:label>
        <skos:definition>inverse of has interaction</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isOtherTherapySpecificsOf -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isOtherTherapySpecificsOf">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/Documents/specifies"/>
        <rdfs:domain rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/OtherTherapySpecifics"/>
