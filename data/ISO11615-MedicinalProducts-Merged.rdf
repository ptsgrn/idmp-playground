    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasPropertyParameters -->


    <owl:DatatypeProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasPropertyParameters">
