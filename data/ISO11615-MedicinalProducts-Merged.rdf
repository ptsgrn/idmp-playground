        <cmns-av:adaptedFrom>ISO 11615:2017 Health informatics - Identification of medicinal products (IDMP) - Data elements and structures for the unique identification and exchange of regulated medicinal product information, clause 9.9.2.6</cmns-av:adaptedFrom>
    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isOutputOf -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isOutputOf">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/RegistrationAuthorities/isRelatedTo"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/"/>
        <rdfs:label>is output of</rdfs:label>
        <skos:definition>is produced by or results from some process, process step, action, activity, or other effort</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isPackageComponentOf -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/isPackageComponentOf">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/Collections/isPartOf"/>
