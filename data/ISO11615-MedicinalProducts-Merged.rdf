        <rdfs:subClassOf rdf:nodeID="genid1394"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/"/>
        <rdfs:label>process step identifier</rdfs:label>
        <skos:definition>identifier assigned to a part of a process</skos:definition>
        <idmp-cmpl:hasConformanceToISOLevel rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/META/ISOConformanceAnnotations/ConformanceToISOLevel-Extension"/>
        <cmns-av:adaptedFrom>ANSI/ISA S88: https://www.isa.org/products/ansi-isa-88-00-01-2010-batch-control-part-1-models (paragraph 3.1.6)</cmns-av:adaptedFrom>
    </owl:Class>
