                    <rdf:Description rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/Stereochemistry-SquarePlanar2"/>
                    <rdf:Description rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/Stereochemistry-SquarePlanar3"/>
